# Zoom RTMS Configuration (Official SDK)
ZM_RTMS_CLIENT=your_zoom_client_id_here
ZM_RTMS_SECRET=your_zoom_client_secret_here

# Webhook Configuration
ZM_RTMS_PORT=8080
ZM_RTMS_PATH=/webhook

# VAD Server
VAD_WS_URL=ws://localhost:8001/vad

# ASR Server
ASR_WS_URL=ws://localhost:8002/asr

# Recording
RECORDING_OUTPUT_DIR=./recordings
ENABLE_RECORDING=true

# Logging
LOG_LEVEL=INFO
